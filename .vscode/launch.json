{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nest Debug",
      "runtimeExecutable": "nest",
      "runtimeArgs": ["start", "--debug", "--watch"],
      "console": "integratedTerminal",
      "restart": true,
      "protocol": "inspector",
      "port": 9229,
      "autoAttachChildProcesses": true,
      "env": {
        "NODE_ENV": "local"
      }
    }
  ]
}
